                "text_xgJnKq": {
                    "type": "text",
                    "settings": {
                        "text": "<p>Obtenez des r\u00e9ponses rapides aux questions fr\u00e9quemment pos\u00e9es.</p>",
                        "text_custom": false,
                        "text_font": "josefin_sans_n4",
